<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name="com.br.xbizitwork.ui.MyApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_logo_xbizit"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo_xbizit_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.XBizWork"
        tools:targetApi="31">
        <activity
            android:name="com.br.xbizitwork.ui.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.XBizWork.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>